{
  "presets": ["@babel/preset-env"],
  "plugins": [
    [
      "babel-plugin-root-import",
      {
        "paths": [
          {
            "rootPathSuffix": "./",
            "rootPathPrefix": "~/"
          },
          {
            "rootPathSuffix": "./server/",
            "rootPathPrefix": "@/"
          }
        ]
      }
    ]
  ]
}